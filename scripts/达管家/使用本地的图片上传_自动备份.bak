		IfColor 721,569 , "E1E1E1", 0 Then
       		Delay 100
     	Else 
    		Exit Do
    	End If
	Loop
	Do
		IfColor 386,398 , "449D44", 0 Then
       		Delay 100
     	Else 
    		Exit Do
